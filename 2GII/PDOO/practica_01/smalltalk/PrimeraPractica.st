Object subclass: #Alumno	instanceVariableNames: 'nombre fechaNacimiento titulacion curso asignaturas'	classVariableNames: ''	poolDictionaries: ''	category: 'PrimeraPractica'!!Alumno methodsFor: 'consultoresBasicos' stamp: 'GMM 11/1/2011 23:38'!getCurso	^curso! !!Alumno methodsFor: 'consultoresBasicos' stamp: 'GMM 11/1/2011 23:38'!getFechaNacimiento	^fechaNacimiento! !!Alumno methodsFor: 'consultoresBasicos' stamp: 'GMM 11/1/2011 23:37'!getNombre	^nombre	"^ devolución del objeto asociado a esa expresión"! !!Alumno methodsFor: 'consultoresBasicos' stamp: 'GMM 11/1/2011 23:38'!getTitulacion	^titulacion! !!Alumno methodsFor: 'modificadoresBasicos' stamp: 'GMM 11/13/2011 21:33'!setAsignaturas	^asignaturas:= OrderedCollection new! !!Alumno methodsFor: 'modificadoresBasicos' stamp: 'GMM 11/2/2011 13:23'!setAsignaturas: unasAsignaturas	^asignaturas:= OrderedCollection new! !!Alumno methodsFor: 'modificadoresBasicos' stamp: 'GMM 11/1/2011 23:41'!setCurso: unCurso	curso:= unCurso! !!Alumno methodsFor: 'modificadoresBasicos' stamp: 'GMM 11/1/2011 23:40'!setFechaNacimiento: unaFechaNacimiento	fechaNacimiento := unaFechaNacimiento! !!Alumno methodsFor: 'modificadoresBasicos' stamp: 'GMM 11/1/2011 23:39'!setNombre: unNombre	nombre := unNombre! !!Alumno methodsFor: 'modificadoresBasicos' stamp: 'GMM 11/1/2011 23:40'!setTitulacion: unaTitulacion	titulacion := unaTitulacion! !!Alumno methodsFor: 'consultores' stamp: 'GMM 11/13/2011 21:52'!asString	^ 'El alumno ', self getNombre ,		', nacido ', self getFechaNacimiento asString,		', se encuentra estudiando ', self getCurso asString,		'º de ', self getTitulacion,		', con las siguientes asignaturas: ', self mostrarMatricula! !!Alumno methodsFor: 'consultores' stamp: 'GMM 11/1/2011 23:43'!cumpleaños	^fechaNacimiento dayOfMonth asString , ' de ' , fechaNacimiento monthName! !!Alumno methodsFor: 'consultores' stamp: 'GMM 11/1/2011 23:42'!edad	^((Date today subtractDate:fechaNacimiento asDate)/365) asFloat truncated! !!Alumno methodsFor: 'comparadores' stamp: 'GMM 11/13/2011 20:46'!esMenorQue: unAlumno	^ self getFechaNacimiento < unAlumno getFechaNacimiento! !!Alumno methodsFor: 'gestionAsignaturas' stamp: 'GMM 11/13/2011 21:06'!estaMatriculado: unCodigoAsignatura	|i encontrado|	i := 1.	encontrado := false.	[encontrado not & (i<= asignaturas size)] whileTrue: [		(((asignaturas at:i) codigo) = unCodigoAsignatura)			ifTrue: [encontrado := true].		i := i+1.	].	^ encontrado! !!Alumno methodsFor: 'gestionAsignaturas' stamp: 'GMM 11/2/2011 13:27'!matricularAsignatura: unaAsignatura	(self estaMatriculado: unaAsignatura codigo)	ifFalse:[asignaturas add: unaAsignatura]	ifTrue:[^'el alumno ya esta matriculado de esa asignatura']! !!Alumno methodsFor: 'gestionAsignaturas' stamp: 'GMM 11/2/2011 13:32'!mostrarMatricula	| result |	result:=''.		asignaturas do: [:i| result:=result,i asString, Character cr asString].	^result! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Alumno class	instanceVariableNames: ''!!Alumno class methodsFor: 'instance creation' stamp: 'GMM 11/13/2011 21:33'!nombre:unNom fechaNacimiento:unaFechaNac titulacion:unaTit curso:unCurso	"expresión de mensaje en cascada"	|alumno|	alumno:=Alumno new.	alumno setNombre: unNom;			setFechaNacimiento: unaFechaNac;			setTitulacion: unaTit;			setCurso: unCurso;			setAsignaturas.	^alumno! !!Alumno class methodsFor: 'instance creation' stamp: 'GMM 11/2/2011 13:22'!nombre:unNom fechaNacimiento:unaFechaNac titulacion:unaTit curso:unCurso asignaturas:unasAsignaturas	"expresión de mensaje en cascada"	|alumno|	alumno:=Alumno new.	alumno setNombre: unNom;			setFechaNacimiento: unaFechaNac;			setTitulacion: unaTit;			setCurso: unCurso;			setAsignaturas: unasAsignaturas.	^alumno! !Object subclass: #Asignatura	instanceVariableNames: 'codigo titulo creditosECTS'	classVariableNames: ''	poolDictionaries: ''	category: 'PrimeraPractica'!!Asignatura methodsFor: 'modificadoresBasicos' stamp: 'GMM 11/2/2011 12:33'!codigo: unCodigo	codigo := unCodigo! !!Asignatura methodsFor: 'modificadoresBasicos' stamp: 'GMM 11/2/2011 12:35'!creditosECTS: cECTS	creditosECTS := cECTS! !!Asignatura methodsFor: 'modificadoresBasicos' stamp: 'GMM 11/2/2011 12:34'!titulo: unTitulo	titulo := unTitulo! !!Asignatura methodsFor: 'consultores' stamp: 'GMM 11/13/2011 21:51'!asString	^ 'Codigo:', self codigo,		' Nombre:', self titulo, 		' Creditos ECTS:', self creditosECTS! !!Asignatura methodsFor: 'consultoresBasicos' stamp: 'GMM 11/2/2011 12:16'!codigo	^codigo! !!Asignatura methodsFor: 'consultoresBasicos' stamp: 'GMM 11/2/2011 12:17'!creditosECTS	^creditosECTS! !!Asignatura methodsFor: 'consultoresBasicos' stamp: 'GMM 11/2/2011 12:17'!titulo	^titulo! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Asignatura class	instanceVariableNames: ''!!Asignatura class methodsFor: 'instance creation' stamp: 'GMM 11/13/2011 20:59'!new: unInteger titulo: unByteString creditosECTS: unFloat	"constructor de Asignatura"	^ (self new) codigo:unInteger; titulo:unByteString; creditosECTS:unFloat! !Object subclass: #PruebaPrimeraPractica	instanceVariableNames: 'asignaturasMatriculadas'	classVariableNames: ''	poolDictionaries: ''	category: 'PrimeraPractica'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PruebaPrimeraPractica class	instanceVariableNames: ''!!PruebaPrimeraPractica class methodsFor: 'pruebas' stamp: 'GMM 11/13/2011 22:09'!prueba1|asignatura1 asignatura2 asignatura3 fecha1 fecha2 alumno1 alumno2 coleccion result|asignatura1 := Asignatura new:1 titulo:'PDOO' creditosECTS:6.asignatura2 := Asignatura new:2 titulo:'FIS' creditosECTS:6.asignatura3 := Asignatura new:3 titulo:'SO' creditosECTS:6.fecha1 := Date year:1970 month:12 day:2.fecha2 := Date year:1980 month:5 day:31.alumno1 := Alumno nombre:'Juan' fechaNacimiento:fecha1 titulacion:'Informatica' curso:2.alumno2 := Alumno nombre:'Ana Maria' fechaNacimiento:fecha2 titulacion:'TITULACION DESCONOCIDA' curso:2.alumno1 matricularAsignatura:asignatura1.alumno1 matricularAsignatura:asignatura2.alumno2 matricularAsignatura:asignatura3.coleccion := OrderedCollection new.coleccion add:alumno1.coleccion add:alumno2.result:=''.		coleccion do: [:i| result:=result,i asString, Character cr asString].^result! !